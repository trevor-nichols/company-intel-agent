{
  "name": "@agenai/feature-company-intel",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./server/*": {
      "types": "./dist/server/*/index.d.ts",
      "import": "./dist/server/*/index.js"
    },
    "./server/services/*": {
      "types": "./dist/server/services/*.d.ts",
      "import": "./dist/server/services/*.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./client/*": {
      "types": "./dist/client/*/index.d.ts",
      "import": "./dist/client/*/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js"
    },
    "./shared/*": {
      "types": "./dist/shared/*/index.d.ts",
      "import": "./dist/shared/*/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm run build:runtime && pnpm run build:types",
    "build:runtime": "tsup",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --outDir dist",
    "dev": "pnpm run build:runtime -- --watch",
    "dev:types": "tsc --project tsconfig.json --emitDeclarationOnly --outDir dist --watch",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test src/server/__tests__/*.test.ts",
    "clean": "rimraf dist *.tsbuildinfo",
    "storybook": "storybook dev --port 6007 --host 0.0.0.0",
    "storybook:build": "storybook build",
    "storybook:test": "storybook test --watch=false"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5",
    "next": "*",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@agenai/logging": "workspace:*",
    "openai": "^6.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.4.1",
    "@storybook/addon-essentials": "^8.4.1",
    "@storybook/addon-interactions": "^8.4.1",
    "@storybook/addon-themes": "^8.4.1",
    "@storybook/addon-actions": "^8.4.1",
    "@storybook/react": "^8.4.1",
    "@storybook/react-vite": "^8.4.1",
    "@storybook/test": "^8.4.1",
    "@storybook/test-runner": "^0.19.1",
    "autoprefixer": "^10.4.20",
    "msw": "^2.6.8",
    "msw-storybook-addon": "^2.0.4",
    "storybook": "^8.4.1",
    "tailwindcss": "^3.4.16",
    "tsup": "^8.5.0",
    "vite": "^5.4.10"
  },
  "engines": {
    "node": ">=18.18"
  }
}
