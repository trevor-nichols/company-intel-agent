{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "dom",
      "dom.iterable",
      "es2022"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": [
      "node"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@company-intel/logging": [
        "./features/company-intel/src/config/logging"
      ],
      "@company-intel/config": [
        "./features/company-intel/src/config/env"
      ],
      "@company-intel/ui": [
        "./features/company-intel/src/ui/primitives/index"
      ],
      "@company-intel/ui/*": [
        "./features/company-intel/src/ui/primitives/*"
      ],
      "@company-intel/ui/lib/*": [
        "./features/company-intel/src/ui/lib/*"
      ],
      "@company-intel/ui/company-intel": [
        "./features/company-intel/src/ui/company-intel/index"
      ],
      "@company-intel/ui/company-intel/*": [
        "./features/company-intel/src/ui/company-intel/*"
      ],
      "@company-intel/shared": [
        "./features/company-intel/src/shared"
      ],
      "@company-intel/shared/*": [
        "./features/company-intel/src/shared/*"
      ],
      "@company-intel/server": [
        "./features/company-intel/src/server/index"
      ],
      "@company-intel/server/*": [
        "./features/company-intel/src/server/*"
      ],
      "@company-intel/feature": [
        "./features/company-intel/src/index"
      ],
      "@company-intel/feature/*": [
        "./features/company-intel/src/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
